@using System.IO;
@model IEnumerable<WebCWMS.ViewModel.ProvinceViewModel>

@{
	Layout = "~/Areas/Identity/Pages/Views/Shared/_LoginPartial.cshtml";
}
@{
	ViewData["Title"] = "Province list";
}

<h3>@ViewData["Title"]</h3>
<hr />
<style>

	/*.aka:hover {
						background: linear-gradient( to right, lch(50% 100 200), lch(50% 100 0) );
						transition: 0.2s;
						}*/

</style>
<form>
	<br>
	<br>
	<table class="table table-responsive  table-hover">
		<thead class="morph">
			<tr class="table-active" style="background: radial-gradient( circle 759px at -6.7% 50%, rgba(80,131,73,1) 0%, rgba(140,209,131,1) 26.2%, rgba(178,231,170,1) 50.6%, rgba(144,213,135,1) 74.1%, rgba(75,118,69,1) 100.3% );
">
				<th>
					Name Of Province
				</th>
			</tr>
		</thead>

		<tbody class="aka">
			@if (Model != null && Model.Any())
			{
				@foreach (var provinceViewModel in Model)
				{
					<tr>
						<td>@provinceViewModel.ProvinceName </td>
					
					</tr>
				}

			}
			else
			{
				<tr class="tr-1">
					<td colspan="9">
						<div>
							<h6 style="text-align:center;">
								No Location availbale at the moment!
							</h6>
						</div>
					</td>
				</tr>
			}
		</tbody>
	</table>

	@*	<div>
	<img src="@("~/Image/" +Model.ImageName)" />

	</div>
	*@
</form>