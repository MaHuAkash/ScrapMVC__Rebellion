	<script>
		$(document).ready(function () {
			// Variable to store the navigation state (visible or hidden)
			var isNavShown = false;

			// Click event handler for the button
			$('.header-btn-collapse-nav').on('click', function () {
				var targetNav = $(this).data('bs-target'); // Get the target nav element using the data attribute
				var $targetNavElement = $(targetNav);

				if (!isNavShown) {
					// If the navigation is hidden, show it
					$targetNavElement.collapse('show');
				} else {
					// If the navigation is shown, hide it
					$targetNavElement.collapse('hide');
				}

				isNavShown = !isNavShown; // Toggle the flag
			});
		});

	</script>

